		Set<Entry<Integer, Character>> es = m.entrySet();
		for (Entry<Integer, Character> en : es) {
			Integer key = en.getKey();
			System.out.println(key);
		}
		Set<Integer> ks = m.keySet();
		System.out.println(cc);
		System.out.println();
	}
	}
	
	
	
